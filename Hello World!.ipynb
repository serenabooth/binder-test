   "source": [
    "def fib(x):\n",
    "    if x == 0: \n",
    "        return 1\n",
    "    elif x == 1:\n",
    "        return 1\n",
    "    else: \n",
    "        return fib(x-1) + fib(x-2)\n"
   ]
